#!/bin/bash
#
# Configuration parameters
#

top_level_domain="<customer>-infra.net"
puppet_tld="dfw01.<customer>.strings-service.net"
data_center="dfw01"
dns_kill_top_zone="1"
dns_email_address="it@example.com"

template_image="xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
base_image="xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
base_image_version="1"

export OS_AUTH_URL=https://identity.api.rackspacecloud.com/v2.0/
export OS_AUTH_SYSTEM=rackspace
export OS_REGION_NAME=DFW
export OS_USERNAME=<username>
export OS_TENANT_NAME=<tenant_id>
export NOVA_RAX_AUTH=1
export OS_PASSWORD=<api_key>
export OS_PROJECT_ID=<tenant_id>
export OS_NO_CACHE=1
